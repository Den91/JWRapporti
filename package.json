{
  "name": "jwrapporti",
  "version": "1.1.2",
  "description": "Rapporti",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-all": "electron-builder build --mac --win --publish never",
    "build-mac": "electron-builder build --mac --publish never",
    "build-win": "electron-builder build --win --publish never",
    "deploy": "electron-builder build --mac --win --publish always"
  },
  "author": "Sita Dennis",
  "license": "ISC",
  "devDependencies": {
    "electron": "^25.2.0",
    "electron-builder": "^23.6.0"
  },
  "dependencies": {
    "bootstrap": "^5.2.2",
    "bootstrap-icons": "^1.9.1",
    "chart.js": "^3.9.1",
    "electron-builder-squirrel-windows": "^24.0.0-alpha.8",
    "electron-db": "^0.15.7",
    "electron-log": "^4.4.8",
    "electron-pdf-window": "^1.0.12",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^5.3.0",
    "jquery": "^3.6.1",
    "node-fpdf": "^1.0.41"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Den91/JWRapporti.git"
  },
  "build": {
    "appId": "com.jwrapporti",
    "directories": {
      "buildResources": "build"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "Den91",
        "repo": "JWRapporti"
      }
    ],
    "mac": {
      "target": "pkg"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "publish": "github",
      "verifyUpdateCodeSignature": false
    }
  }
}
